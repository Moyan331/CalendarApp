{
  "expo": {
    "name": "CalendarApp",
    "slug": "CalendarApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "calendarapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "icon":"./assets/images/icon.png",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.mmmoyan.CalendarApp"
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}